-- dialog system

-- is a dialog currently playing?
dialog_on = false

function unpack_split(s)
    return unpack(split(s))
end

-- start next dialog
function dialog_start()
 -- if dialog not declared, set it to empty
 if not dialog then
  dialog = {}
 end
 -- current line and char in dialog
 dialog_l, dialog_c = 0, 0
 dialog_on = true
 dialog_next()
end

-- move to the next dialog text.
-- returns true if the dialog has ended.
function dialog_next()
 dialog_l += 1
 -- read portrait numbers
 while type(dialog[dialog_l]) == "number" do

  -- current dialog portrait
  dial_portrait = dialog[dialog_l]
  -- duration of the portrait so far
  portrait_dur = 0
  -- does the dialog line clear the screen? (black if it has a .1 decimal value, white if .2)
  dial_clear_col = nil
  -- does the dialog automatically starts the next one once finished?
  dial_yapping = false

  -- special dialog modifiers
  local modif = dial_portrait%1
  if modif == .1 then
   dial_clear_col = 0
  elseif modif == .2 then
   dial_clear_col = 7
  elseif modif == .3 then
   dial_yapping = true
  end

  dialog_l += 1
 end
 dialog_c = 0
 portrait_dur += 1

 if dialog_l > #dialog then
  -- end the dialog, continue with the game
  dialog_on = false
  frame = -1
  if level_music != level_dialog_music then
   music(level_music, 0, 3)
  end
  return true
 end
 return false
end

-- update the displaying of the dialog system
-- note: this will use sfx(63) for dialog beeps.
function dialog_update()
 local incomplete = dialog_c < #dialog[dialog_l]
 if btnp(❎) or btn(🅾️) then
  if incomplete and not btn(🅾️) then
   dialog_c = 200
  else
   dialog_next()
  end
  sfx(63)
 elseif incomplete then
  dialog_c += 1
  sfx(63)
 elseif dial_yapping then
  dialog_next()
 end

 -- dialogextra()
end

-- draw the dialog's portrait using the value of dial_portrait.
function draw_portrait()
 local s = dial_portrait
 if s >= 10 then
  local spriteswitch = 0
  local chara, expre = s\10 - 1, s%10 - 1
  spr(64 + 2 * chara, 1, 108, 2, 2)
  if expre >= 0 then
   if s >= 30 then
    spriteswitch = 2
   end
   rectfill(unpack_split"6,116,13,123,0")
   spr(96 + expre + 16 * (chara - spriteswitch) + 2 * spriteswitch, 6, 116)
  end
 end
end

-- draw the current dialog
function dialog_draw()
 if not dialog_on then
  return
 end

 -- clear the screen black if the dialog number requires it
 if dial_clear_col then
  cls(dial_clear_col)
 end

 rectfill(unpack_split"0,106,127,127,0")
 rect(unpack_split"0,107,17,124,7")
 rect(unpack_split"19,107,127,124,7")

 -- draw next dialog icon
 print("❎", 120, 123, 2)
 print("❎", 120, 122 + (frame\8)%2, 8)

 -- show the substring of the current dialog line
 draw_portrait()
 local text1 = sub(dialog[dialog_l], 1, dialog_c)
 print(text1, 21, 109, 10)
end

