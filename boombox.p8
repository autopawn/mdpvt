-- distortion shader

function distort_screen(d)
 for i=1,flr(30*d+rnd(1.2)) do
  local y = flr(rnd(128))
  local size = flr(rnd(10*d))
  local offset = 1 + flr(rnd(5*d))
  if rnd() < 0.5 then
   -- left-to-right
   for row=y,y+size do
    for x=0,127 do
     pset(x, row, pget(x + offset, row))
    end
   end
  else
   -- right-to-left
   for row=y,y+size do
    for x=127,0,-1 do
     pset(x, row, pget(x - offset, row))
    end
   end
  end
 end
end
