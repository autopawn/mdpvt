-- logic

-- songs that fit in the display
listsize = 6

function play_song(n)
 last_stat54 = -1
 last_stat56 = 0
 loops = 0
 option_playing = n
 if n > 0 then
  local s = songs[n][1]
  music(s)
 else
  music(-1)
 end
 playing_t = 0
end

function _init()
 play_song(0)
 option = 1
 offset = 1
 option_t = 0
 mwaves_init()
 distortion = 1.0
 dialog_start()
 frame = 0
end

function _update60() 
 frame += 1
 if dialog_on then
  dialog_update()
  return
 end

 -- loop detection
 local s = stat(54) -- current music pattern
 local t = stat(56) -- number of ticks in the current pattern
 if s < last_stat54
   or (s == last_stat54
   and t < last_stat56) then
  loops += 1
 end
 last_stat54 = s
 last_stat56 = t
 -- stop at the end of the 2nd loop
 if loops >= 2 then
  music(-1)
 end

 if btnp(⬆️) then
  option -= 1
  option_t = 0
  offset = min(offset, option - 1)
  distortion += 0.35
  sfx(62)
 end

 if btnp(⬇️) then
  option += 1
  option_t = 0
  offset = max(offset, option + 2 - listsize)
  distortion += 0.35
  sfx(62)
 end

 if btnp(❎) then
  if option == option_playing then
   if stat(24) >= 0 then
    music(-1) -- stop
    sfx(61)
   else
    play_song(option)
   end
  else
   play_song(option)
  end
 end
 option = max(1, option)
 option = min(option, #songs)
 offset = min(offset, #songs - listsize + 1)
 offset = max(1, offset)
 
 option_t += 1
 playing_t += 1

 mwaves_update()
 
 distortion += mwaves_avg_volume()/70 
 distortion = min(distortion, 1)
 distortion *= 0.75
end

