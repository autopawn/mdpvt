-- aux

-- get a string as it should
-- be displayed on a limited
-- buffer of size len, after
-- f frames
function partial_sub(txt, len, f)
 local n = #txt
 if n < len then
  return txt
 end
 local window = n - len + 1
 local cycle = 48 + 8*window + 48
 f %= cycle

 local i
 if f < 48 then
  i = 1
 elseif f >= cycle - 48 then
  i = window
 else
  i = 1 + flr((f - 48)/8)
 end
 return sub(txt, i, i+len-1)
end
