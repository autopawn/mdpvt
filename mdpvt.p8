--player
-- this tab is for the player's
-- behavior.

pla = {}
pla.x = 64
pla.y = 64
pla.vx = 0
pla.vy = 0
pla.animb = 0 -- body animation
pla.animw = 0 -- wing animation
pla.w = 8 -- width
pla.h = 16 -- height
pla.facer = true -- facing right?
pla.ground = false -- touching ground
pla.deadt = 0 -- time the player is dead

function playerupdate()
 if pla.deadt > 0 then
  -- if the player is dead:
  pla.deadt += 1
  pla.h = 8
  
  -- reset the game
  if pla.deadt > 60 then
   run()
  end
 else
  -- update wing animation
  pla.animw -= 1
 
  if btn(0) then
   pla.vx -= 1
   pla.animb += 1
   pla.facer = false
   if pla.ground then
    sfx(3)
   end  end
  if btn(1) then
   pla.vx += 1
   pla.animb += 1
   pla.facer = true
   if pla.ground then
    sfx(3)
   end
  end
  if not btn(0) and not btn(1) then
   pla.vx /= 4
  end
  if btnp(2) then
   pla.vy -= 3
   pla.animw = 7
  end
 
  -- limit horizontal speed
  if pla.vx > 4 then
   pla.vx = 4
  end
  if pla.vx < -4 then 
   pla.vx = -4
  end

  -- limit vertical speed
  if pla.vy > 7 then
   pla.vy = 7
  end
  if pla.vy < -4 then 
   pla.vy = -4
  end
  
  -- update body animation
  if not pla.ground then
   pla.animb = 0
  end
 end
 
 objmove(pla) 
 pla.vy += 0.2 -- gravity
 
 -- die if touching death block
 if pla.deadt==0 and
   rectcol(pla.x, pla.y, pla.w,
   pla.h, 1) then
  pla.deadt = 1
  pla.deadx = pla.x
  pla.deady = pla.y
  pla.vy -= 2
  sfx(0)
 end
end

function playerdraw()
 fr = pla.facer

 if pla.deadt > 0 then
  t = pla.deadt
  
  -- draw head
  spr(18, pla.x, pla.y,
	 		1, 1, fr)
	 		
	 -- draw explosion
	 for dx=-2,2 do
	  for dy=-2,2 do
	   if dy%2!=0 or dx%2!=0 then
	    circfill(
	      pla.deadx+3+dx*t,
	      pla.deady+10+dy*t,
	      1+3*cos(t/8), 6)
	   end
	  end
	 end
 else
	 -- draw head
	 if frame % 60 < 4 then
	  headspr = 17 -- blink
	 else
	  headspr = 16
	 end
	 spr(headspr, pla.x, pla.y,
	 		1, 1, fr)
 
	 -- draw wingsplayerdraw
	 if pla.animw >= 0 then
 	 spr(48+pla.animw/2,
 	   pla.x-6, pla.y+5, 1, 1, true)
 	 spr(48+pla.animw/2,
 	   pla.x+6, pla.y+5, 1, 1, false)
	 end
 
	 -- draw body
	 bodyspr = 32+pla.animb%4		
 	spr(bodyspr, pla.x, pla.y+8,
  	 1, 1, fr)
 end
end
