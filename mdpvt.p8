--worker drones
--this tab is for declaring and
--adding worker drones

workers={}
function create_worker(x1,y1)
  local worker = {
    x=x1,
    vx=0,
    y=y1,
    vy=0,
    spr=20,
    bodyspr=36,
    deadspr=21,
    deadbodyspr=37,
    h=16,
    w=8,
    dead=false
  }
  add(workers,worker)
end


function worker_update()
	for worker in all(workers) do
		if (objcol(pla,worker)) and (pla.deadt < 1 ) and (worker.dead == false) then
      sfx(2)
			worker.dead=true
      worker.vy+=0.5
      worker.h=8
      add_blood(worker.x,worker.y,rnd(6)+10)
    end
    -- limit vertical speed
    if worker.vy > 7 then
      worker.vy = 7
    end
    if worker.vy < -4 then 
      worker.vy = -4
    end
    objmove(worker)
    worker.vy += 0.2 -- gravity
	end
end

function workers_draw()
  for worker in all(workers) do
    if (worker.dead == true) then
      spr(worker.deadspr,worker.x,worker.y,1,1)
      spr(worker.deadbodyspr,worker.x-7,worker.y,1,1)
    else
      spr(worker.spr,worker.x,worker.y,1,1)
      spr(worker.bodyspr,worker.x,worker.y+8,1,1)
    end
  end
end

particles={}
function add_blood(x,y,amount)
  for i=0,amount do
    add(particles,{
      x=x,
      vx=rnd(2)-1,
      y=y,
      vy=rnd(0.75)-0.25,
      size=rnd(1)+1,
      w=1,
      h=1,
      color=8,
      lifetime=rnd(60)+50,
    })
  end
end

function update_particles()
  for p in all(particles) do
    if p.lifetime < 0 then
      del(particles,p)
    end
    if p.vx > 4 then
      p.vx = 4
     end
     if p.vx < -4 then 
      p.vx = -4
     end
   
     -- limit vertical speed
     if p.vy > 7 then
      p.vy = 7
     end
     if p.vy < -4 then 
      p.vy = -4
     end
    objmove(p)
    p.vy += 0.05
    if p.vx > 0 then
      p.vx -= 0.01
    else 
      p.vx += 0.01
    end
    p.lifetime -= 1
  end
end

function draw_particles()
  for p in all(particles) do
    circfill(p.x,p.y,p.size,p.color)
  end
end
