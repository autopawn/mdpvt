--draw
frame = 0

function _init()
  for x=0,150 do
    for y=0,150 do
      if mget(x,y)==22 then
        create_worker(x*8,y*8)
        print("created worker")
        mset(x,y,0)
      end
    end
  end
end

function _update()
 playerupdate()
 worker_update()
 update_particles()
 -- update frame counter
 frame += 1
end

function _draw()
  cls()
  map()
  workers_draw()
  draw_particles()

  camera(pla.x-64+3, pla.y-64+8)
  playerdraw()
end


